#include <stdio.h>
#include <unistd.h>

extern char **environ;

/**
 * main - program that prints the environment
 *        using the global variable environ.
 *
 * Return: 0, Success.
 */
int main(int ac, char **av, char **env)
{
    unsigned int i;

    i = 0;
    while (env[i] != NULL)
    {
        printf("%s\n", env[i]);
        i++;
    }
    return (0);
}
